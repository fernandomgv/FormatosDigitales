    <cms_query>
      <QueryID>14211</QueryID>
      <QueryName>GetPaisDepartamentobyCode</QueryName>
      <QueryTypeID>0</QueryTypeID>
      <QueryText>SELECT ##COLUMNS## 
FROM  CMS_Country c
    left outer join CMS_State s
	on s.CountryID=c.CountryID
        and s.StateName=@StateName
where c.CountryName=@CountryName</QueryText>
