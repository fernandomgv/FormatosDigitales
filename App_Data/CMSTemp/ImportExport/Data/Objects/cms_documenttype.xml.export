    <cms_query>
      <QueryID>14014</QueryID>
      <QueryName>selectlatest</QueryName>
      <QueryTypeID>0</QueryTypeID>
      <QueryText>SELECT TOP 3 View_CMS_Tree_Joined.*, CONTENT_News.* 
FROM 
   View_CMS_Tree_Joined INNER JOIN CONTENT_News ON View_CMS_Tree_Joined.DocumentForeignKeyValue = CONTENT_News.NewsID 
WHERE 
   ClassName = 'CMS.News' 
AND 
   NewsReleaseDate &lt;= GetDate() AND Published = 1 
ORDER BY NewsReleaseDate DESC, NewsTitle</QueryText>
